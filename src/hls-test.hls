function serialize_env_path(path type string) : result type list {
    var item type string;
    var p type int;
    var path_lst type list;
    $p = 0;
    while ($p < $path.len()) {
        if ($path.at($p) == ";" && $item.len() > 0) {
            $path_lst.add_item($item);
            $item = "";
        } else {
            if ($path.at($p) != ";") $item = $item + $path.at($p);
        }
        $p = $p + 1;
    }
    if ($item.len() > 0) { # Oh... ;)
        $path_lst.item($item);
    }
    result $path_lst;
}

