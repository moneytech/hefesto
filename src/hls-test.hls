function get_c_cpp_deps_() : result type string {
    var files type list;
    var includes type list;
    var f type int;
    var i type int;
    var dep_chain type string;
    var str type string;
    var cwd type string;
    var exts type list;
    var e type int;
    var dep_nr type int;

    #GPF na lib here:
    #$files.ls(".*\\.(h|hpp)$");
    #$str.replace(".*(\"|>)$", "");
    #$files.ls(".*\\." + $exts.item($e) + "$"); NULL ap, a expressao nao esta pos-fixada, tentar adotar aquela abordagem geral de deixar tudo pronto e nao se preocupar mais com isso durante a interpretacao

    $e = 0;

    $exts.add_item("h");
    $exts.add_item("hpp");
    $exts.add_item("c");
    $exts.add_item("C");
    $exts.add_item("CC");
    $exts.add_item("cc");
    $exts.add_item("cpp");

#    $str.replace(".*", ("blah" + "bla") + "h" + "!");

    while ($e < $exts.count()) {
#        $str = ".*\\." + $exts.item($e) + "$";
        $files.ls(".*\\." + $exts.item($e) + "$");
        $f = 0;
        $cwd = hefesto.sys.pwd();
        while ($f < $files.count()) {
#            hefesto.sys.echo("NOW SCANNING = " + $files.item($f) + "\n");
            $str = $files.item($f);
            $dep_chain = $dep_chain + filenamefrompath($str) + ": ";
            $includes = hefesto.sys.lines_from_file($str, "#include.*$");
            $i = 0;
            $dep_nr = 0;
            while ($i < $includes.count()) {
                $str = $includes.item($i);
                $str.replace(".*#include.*(\"|<)", "");
                $str.replace("(\"|>)$", "");
                if (isfile($str) == 1) {
                    $str = hefesto.sys.make_path($cwd, $str);
                    if ($dep_nr > 0) {
                        $dep_chain = $dep_chain + "& ";
                    }
                    $dep_chain = $dep_chain + $str + " ";
                    $dep_nr = $dep_nr + 1;
                }
                $i = $i + 1;
            }
            $dep_chain = $dep_chain + ";\n";
            $f = $f + 1;
        }
        $files.clear();
        $e = $e + 1;
    }
    result $dep_chain;
}

